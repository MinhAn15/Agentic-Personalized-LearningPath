  "paths": {
    "/api/v1/agents/knowledge-extraction": {
      "post": {
        "tags": [
          "agents"
        ],
        "summary": "Extract Knowledge",
        "description": "Extract learning concepts from an educational document.\n\nThis endpoint:\n1. Parses the document\n2. Extracts key concepts using LLM\n3. Identifies prerequisites\n4. Creates nodes/relationships in Neo4j Course KG\n\nExample request:\n{\n    \"content\": \"SQL tutorial content...\",\n    \"document_type\": \"LECTURE\",\n    \"title\": \"SQL Basics\"\n}\n\nExample response:\n{\n    \"success\": true,\n    \"agent_type\": \"knowledge_extraction\",\n    \"execution_time_ms\": 2345.5,\n    \"result\": {\n        \"document_id\": \"doc_123\",\n        \"concepts_extracted\": 8,\n        \"concepts_created\": 8,\n        \"relationships_created\": 5\n    }\n}",
        "operationId": "extract_knowledge_api_v1_agents_knowledge_extraction_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DocumentInput"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AgentExecutionResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/agents/profiler": {
      "post": {
        "tags": [
          "agents"
        ],
        "summary": "Profile Learner",
        "description": "Create a learner profile from natural language input.\n\nThis endpoint:\n1. Parses learner's message\n2. Extracts goal, timeline, learning style\n3. Creates learner profile\n4. Initializes Personal KG\n5. Saves to PostgreSQL\n\nExample request:\n{\n    \"message\": \"I want to learn SQL JOINs in 2 weeks. Already know SELECT/FROM.\",\n    \"learner_name\": \"John\"\n}\n\nExample response:\n{\n    \"success\": true,\n    \"agent_type\": \"profiler\",\n    \"execution_time_ms\": 1234.5,\n    \"result\": {\n        \"learner_id\": \"user_abc123\",\n        \"goal\": \"Master SQL JOINs\",\n        \"time_available\": 14,\n        \"learning_style\": \"VISUAL\"\n    }\n}",
        "operationId": "profile_learner_api_v1_agents_profiler_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LearnerInput"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AgentExecutionResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
