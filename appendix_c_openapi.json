  "components": {
    "schemas": {
      "AgentExecutionResponse": {
        "properties": {
          "success": {
            "type": "boolean",
            "title": "Success"
          },
          "agent_type": {
            "type": "string",
            "title": "Agent Type"
          },
          "execution_time_ms": {
            "type": "number",
            "title": "Execution Time Ms"
          },
          "result": {
            "additionalProperties": true,
            "type": "object",
            "title": "Result"
          },
          "error": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Error"
          }
        },
        "type": "object",
        "required": [
          "success",
          "agent_type",
          "execution_time_ms",
          "result"
        ],
        "title": "AgentExecutionResponse",
        "description": "Response from agent execution"
      },
      "DocumentInput": {
        "properties": {
          "content": {
            "type": "string",
            "title": "Content"
          },
          "document_type": {
            "$ref": "#/components/schemas/DocumentType",
            "default": "LECTURE"
          },
          "title": {
            "type": "string",
            "title": "Title",
            "default": ""
          },
          "source_url": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Source Url"
          },
          "force_real": {
            "type": "boolean",
            "title": "Force Real",
            "default": false
          }
        },
        "type": "object",
        "required": [
          "content"
        ],
        "title": "DocumentInput",
        "description": "Input document for knowledge extraction"
      },
      "DocumentType": {
        "type": "string",
        "enum": [
          "LECTURE",
          "TUTORIAL",
          "TEXTBOOK",
          "EXERCISE",
          "REFERENCE"
        ],
        "title": "DocumentType",
        "description": "Types of learning documents"
      },
      "EvaluationInput": {
        "properties": {
          "learner_id": {
            "type": "string",
            "title": "Learner Id"
          },
          "concept_id": {
            "type": "string",
            "title": "Concept Id"
          },
          "learner_response": {
            "type": "string",
            "title": "Learner Response"
          },
          "expected_answer": {
            "type": "string",
            "title": "Expected Answer"
          },
          "correct_answer_explanation": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Correct Answer Explanation"
          },
          "force_real": {
            "type": "boolean",
            "title": "Force Real",
            "default": false
          }
        },
        "type": "object",
        "required": [
          "learner_id",
          "concept_id",
          "learner_response",
          "expected_answer"
        ],
        "title": "EvaluationInput",
        "description": "Input for Evaluator Agent"
      },
      "EvaluationResponse": {
        "properties": {
          "success": {
            "type": "boolean",
            "title": "Success"
          },
          "learner_id": {
            "type": "string",
            "title": "Learner Id"
          },
          "concept_id": {
            "type": "string",
            "title": "Concept Id"
          },
          "score": {
            "type": "number",
            "title": "Score"
          },
          "error_type": {
            "type": "string",
            "title": "Error Type"
          },
          "misconception": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Misconception"
          },
          "feedback": {
            "type": "string",
            "title": "Feedback"
          },
          "decision": {
