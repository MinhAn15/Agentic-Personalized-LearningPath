    "/api/v1/paths/plan": {
      "post": {
        "tags": [
          "paths"
        ],
        "summary": "Plan Learning Path",
        "description": "Plan optimal learning path for a learner.\n\nThis endpoint:\n1. Gets learner profile\n2. Gets Course KG from Neo4j\n3. Uses RL to select optimal concept sequence\n4. Generates day-by-day path with resource recommendations\n\nExample request:\n{\n    \"learner_id\": \"user_123\",\n    \"goal\": \"Master SQL JOINs\"\n}\n\nExample response:\n{\n    \"success\": true,\n    \"learner_id\": \"user_123\",\n    \"learning_path\": [\n        {\n            \"day\": 1,\n            \"concept\": \"SQL_WHERE\",\n            \"difficulty\": 2,\n            \"estimated_hours\": 4\n        },\n        ...\n    ],\n    \"pacing\": \"MODERATE\",\n    \"success_probability\": 0.92\n}",
        "operationId": "plan_learning_path_api_v1_paths_plan_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PlanPathRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
